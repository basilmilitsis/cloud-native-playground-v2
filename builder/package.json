{
  "name": "builder",
  "version": "0.0.1",
  "description": "build CLI",
  "private": true,
  "types": ".build/types/types.d.ts",
  "bin": {
    "builder": "bin/builder"
  },
  "scripts": {
    "build:format": "prettier --write **/*.{js,ts,tsx,json}",
    "build:lint": "tslint -p .",
    "build:clean-build": "rm -rf ./.build",
    "build:compile": "tsc -p .",
    "build:copy-templates":
      "if [ -e ./src/templates ]; then cp -a ./src/templates ./.build/; fi",
    "build":
      "yarn build:format && yarn build:clean-build && yarn build:compile && yarn build:copy-templates",
    "dev:global:link": "npm link",
    "dev:global:unlink": "npm unlink builder",
    "prepublishOnly": "yarn build",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "test:snapupdate": "jest --updateSnapshot",
    "test:coverage": "jest --coverage"
  },
  "files": [
    "tsconfig.json",
    "tslint.json",
    "build",
    "LICENSE",
    "readme.md",
    "docs",
    "bin"
  ],
  "license": "MIT",
  "dependencies": {
    "@types/js-yaml": "3.12.5",
    "@types/pg": "7.14.7",
    "dotenv": "8.2.0",
    "gluegun": "latest",
    "js-yaml": "3.14.1",
    "pg": "8.0.3",
    "pg-copy-streams": "5.1.1",
    "typeorm": "0.2.29",
    "typeorm-naming-strategies": "2.0.0"
  },
  "devDependencies": {
    "@types/jest": "24.0.18",
    "@types/node": "12.7.11",
    "jest": "24.1.0",
    "prettier": "1.12.1",
    "ts-jest": "24.1.0",
    "ts-node": "8.4.1",
    "tslint": "5.12.0",
    "tslint-config-prettier": "1.17.0",
    "tslint-config-standard": "8.0.1",
    "typescript": "3.6.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "resolutions": {
    "@types/babel__traverse": "7.0.6"
  }
}
