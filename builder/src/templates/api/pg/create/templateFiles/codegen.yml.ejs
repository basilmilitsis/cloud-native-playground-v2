schema: './src/db/.generated/schema/db.graphql'
documents: ['./src/db/queries/**/*.ts', './src/db/mutations/**/*.ts']
generates:
  src/db/.generated/typings/DbTypes.ts:
    plugins:
      - add: 
          content: '// WARNING: This file is automatically generated. Do not edit.'
      - typescript
      - typescript-operations
    config:
      skipTypename: true