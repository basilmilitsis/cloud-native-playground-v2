{
    "name": "<%= props.name %>",
    "version": "1.0.0",
    "description": "",
    "main": "./.build/index.js",
    "scripts": {
        "compile": "tsc",
        "compile:db-models": "npx tsc -p ./tsconfig-init-db.json",
        "compile:sync": "npx tsc -p ./tsconfig-sync.json",
        "compile:watch": "tsc -w -p .",
    
        "init:db": "npm run compile:db-models && build api pg init-db",

        "sync:db-schema": "npm run compile:sync && node -r dotenv/config ./.build_sync/indexSync.js dotenv_config_path=./../../.env",
    
        "generate:db:code:typings": "graphql-codegen",
        "generate:db:code:queries": "build api pg generate-db-code-queries",
        "generate:db:code:mutations": "build api pg generate-db-code-mutations",
        "generate:db:code": "npm run generate:db:code:typings && npm run generate:db:code:queries && npm run generate:db:code:mutations",
    
        "generate:api:code:api-builder": "build api pg generate-api-builder",
    
        "start:dev": "npm run compile && node -r dotenv/config ./.build/index.js dotenv_config_path=./../../.env",
        "start:dev:debug": "nodemon --legacy-watch --watch ./.build --inspect=0.0.0.0:5858 --nolazy ./.build/index.js",
        "start": "node ./.build/index.js",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
